LOAD DATABASE
     FROM      mysql://usr:pwd@127.0.0.1/kp_db
     INTO postgresql://usr:pwd@127.0.0.1:5432/crm

 WITH include drop, create tables, create indexes, reset sequences,
      workers = 8, concurrency = 1,
      multiple readers per thread, rows per range = 50000

 -- regular expression matches only tables starting OL_
 INCLUDING ONLY TABLE NAMES MATCHING ~/OL_/

ALTER TABLE NAMES MATCHING ~/OL_/
   SET SCHEMA 'public';
