													<xs:element name="room_name" minOccurs="1" maxOccurs="1">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	The name of the room
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:string">
																<xs:minLength  value="1"/>
																<xs:maxLength  value="120"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="room_description" minOccurs="0" maxOccurs="1">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	The specific description of that room
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:string">
																<xs:maxLength  value="1000"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="room_length" minOccurs="0" maxOccurs="1">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	The length of the room
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:decimal">
																<xs:minInclusive value="0"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="room_width" minOccurs="0" maxOccurs="1">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	The width of the room
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:decimal">
																<xs:minInclusive value="0"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="room_dimension_unit" minOccurs="0" maxOccurs="1">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	The units which the length and width of the room have been provided in: 5 Metres, 6 Centimetres, 7 Millimetres, 8 Feet, 9 Inches
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:int">
																<xs:enumeration value="5"/>
																<xs:enumeration value="6"/>
																<xs:enumeration value="7"/>
																<xs:enumeration value="8"/>
																<xs:enumeration value="9"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="room_dimension_text" minOccurs="0" maxOccurs="1">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	Any additional description or clarification of the room dimensions
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:string">
																<xs:minLength  value="1"/>
																<xs:maxLength  value="120"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="room_photo_urls" minOccurs="0" maxOccurs="10">
														<xs:simpleType >
															<xs:annotation>
																<xs:documentation>
																	The URLs of images which should be associated with this room
																</xs:documentation>
															</xs:annotation>
															<xs:restriction base="xs:string">
																<xs:maxLength  value="250"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
											<xs:unique name="room_photo_urls_unique">
												<xs:selector xpath="room_photo_urls" />
												<xs:field xpath="." />
											</xs:unique>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
								<xs:unique name="features_unique">
									<xs:selector xpath="features" />
									<xs:field xpath="." />
								</xs:unique>
								<xs:unique name="parking_unique">
									<xs:selector xpath="parking" />
									<xs:field xpath="." />
								</xs:unique>
								<xs:unique name="outside_space_unique">
									<xs:selector xpath="outside_space" />
									<xs:field xpath="." />
								</xs:unique>
								<xs:unique name="accessibility_unique">
									<xs:selector xpath="accessibility" />
									<xs:field xpath="." />
								</xs:unique>
								<xs:unique name="heating_unique">
									<xs:selector xpath="heating" />
									<xs:field xpath="." />
								</xs:unique>
								<xs:unique name="comm_use_class_unique">
									<xs:selector xpath="comm_use_class" />
									<xs:field xpath="." />
								</xs:unique>
							</xs:element>

							<xs:element name="media" minOccurs="0" maxOccurs="999">
								<xs:annotation>
									<xs:documentation>
										The media for the property being sent
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="media_type" minOccurs="1" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The type of media which is being sent: 1 Image, 2 Floorplan, 3 Brochure, 4 Virtual Tour, 5 Audio Tour, 6 EPC, 7 EPC Graph
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:int">
													<xs:enumeration value="1"/>
													<xs:enumeration value="2"/>
													<xs:enumeration value="3"/>
													<xs:enumeration value="4"/>
													<xs:enumeration value="5"/>
													<xs:enumeration value="6"/>
													<xs:enumeration value="7"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="media_url" minOccurs="1" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The URL to retrieve this piece of media from
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="1"/>
													<xs:maxLength  value="250"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="caption" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The caption to be displayed for this piece of media
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:maxLength  value="50"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="sort_order" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The display order for this piece of media
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:int">
													<xs:minInclusive value="0"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="media_update_date" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The date the media at this URL was last updated in the format: dd-MM-yyyy HH:mm:ss
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string"/>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="principal" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>
										Details of the principal for the property being sent
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="principal_email_address" minOccurs="1" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The email address for the principal for this property
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="6"/>
													<xs:maxLength  value="200"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="auto_email_when_live" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														Should the principal be emailed when this property is live on Rightmove
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:boolean"/>
											</xs:simpleType>
										</xs:element>
										<xs:element name="auto_email_updates" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														Should the principal be emailed when this property is updated on Rightmove
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:boolean"/>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>