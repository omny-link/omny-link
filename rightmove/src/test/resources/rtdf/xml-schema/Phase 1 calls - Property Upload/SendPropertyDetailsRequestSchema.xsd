							<xs:element name="status">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											The current transaction status for this property: 1 Available, 2 SSTC (sales only), 3 SSTCM (Scottish sales only), 4 Under Offer (sales only), 5 Reserved (sales only), 6 Let Agreed (lettings only)
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:int">
										<xs:enumeration value="1"/>
										<xs:enumeration value="2"/>
										<xs:enumeration value="3"/>
										<xs:enumeration value="4"/>
										<xs:enumeration value="5"/>
										<xs:enumeration value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="new_home" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Defines whether this property is a new build
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:boolean"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="student_property" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Defines whether this property is available for student lettings
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:boolean"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="house_flat_share" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Defines whether this advert is for a house/flat share
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:boolean"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="create_date" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Date this property was created in the format: dd-MM-yyyy HH:mm:ss
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="update_date" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Date this property was last updated in the feed provider's software, in the format: dd-MM-yyyy HH:mm:ss
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="date_available" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Date a rental property is available in the format: dd-MM-yyyy
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="contract_months" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Length of rental contract in months
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:int">
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="minimum_term" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											Minimum term for the rental contract in months
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:int">
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="let_type" minOccurs="0" maxOccurs="1">
								<xs:simpleType >
									<xs:annotation>
										<xs:documentation>
											The type of rental contract available for this property: 1 Long term, 2 Short term, 4 Commercial, 0 Not specified
										</xs:documentation>
									</xs:annotation>
									<xs:restriction base="xs:int">
										<xs:enumeration value="1"/>
										<xs:enumeration value="2"/>
										<xs:enumeration value="4"/>
										<xs:enumeration value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="address">
								<xs:annotation>
									<xs:documentation>
										Information about the address of the property being sent
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="house_name_number">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The first line of the address including the property name or number
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="1"/>
													<xs:maxLength  value="60"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="address_2" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The second line of the address for this property
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:maxLength  value="60"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="address_3" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The third line of the address for this property
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:maxLength  value="60"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="address_4" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The fourth line of the address for this property
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:maxLength  value="60"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="town">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The town in which the property is located
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="1"/>
													<xs:maxLength  value="60"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="postcode_1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The first half of the post code for the property being sent
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="1"/>
													<xs:maxLength  value="4"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="postcode_2">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The second half of the post code for the property being sent
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="1"/>
													<xs:maxLength  value="3"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="display_address">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The address which should be displayed for the property being sent
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:string">
													<xs:minLength  value="1"/>
													<xs:maxLength  value="120"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="latitude" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The exact latitude of the property
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-90"/>
													<xs:maxInclusive value="90"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="longitude" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The exact longitude of the property
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-180"/>
													<xs:maxInclusive value="180"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="pov_latitude" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The latitude for the google streetview camera
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-90"/>
													<xs:maxInclusive value="90"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="pov_longitude" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The longitude for the google streetview camera
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-180"/>
													<xs:maxInclusive value="180"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="pov_pitch" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The pitch for the google streetview camera
													</xs:documentation>
												</xs:annotation>
                                                <xs:restriction base="xs:decimal">
                                                    <xs:minInclusive value="-180"/>
                                                    <xs:maxInclusive value="180"/>
                                                </xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="pov_heading" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The heading for the google streetview camera
													</xs:documentation>
												</xs:annotation>
                                                <xs:restriction base="xs:decimal">
                                                    <xs:minInclusive value="-360"/>
                                                    <xs:maxInclusive value="360"/>
                                                </xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="pov_zoom" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The zoom level for the google streetview camera
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:int"/>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="price_information">
								<xs:annotation>
									<xs:documentation>
										Information about the price of the property being sent
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="price">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The price of the property being sent
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="price_qualifier" minOccurs="0" maxOccurs="1">
											<xs:simpleType >
												<xs:annotation>
													<xs:documentation>
														The qualifier on the advertised price of the property being sent: 0 Default, 1 POA, 2 Guide Price, 3 Fixed Price, 4 Offers in excess of, 5 OIRO, 6 Sale by Tender, 7 From, 9 Shared Ownership, 10 Offers over, 11 Part Buy, Part Rent, 12 Shared Equity, 16 Coming Soon
													</xs:documentation>
												</xs:annotation>
												<xs:restriction base="xs:int">
													<xs:enumeration value="0"/>
													<xs:enumeration value="1"/>
													<xs:enumeration value="2"/>
													<xs:enumeration value="3"/>
													<xs:enumeration value="4"/>
													<xs:enumeration value="5"/>
													<xs:enumeration value="6"/>
													<xs:enumeration value="7"/>
													<xs:enumeration value="9"/>
													<xs:enumeration value="10"/>
													<xs:enumeration value="11"/>
													<xs:enumeration value="12"/>
                                                    <xs:enumeration value="16"/>
												</xs:restriction>
