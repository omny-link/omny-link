    <serviceTask activiti:class="com.knowprocess.resource.spi.RestGet" completionQuantity="1" id="getMemo" implementation="##WebService" isForCompensation="false" name="Read memo" startQuantity="1">
      <extensionElements>
        <activiti:field name="resourceUsername">
          <activiti:string>userInfo('tenant-bot')</activiti:string>
        </activiti:field>
        <activiti:field name="resourcePassword">
          <activiti:expression>userInfo('cust-mgmt-secret')</activiti:expression>
        </activiti:field>
        <activiti:field name="globalResource">
          <activiti:expression>userInfo('cust-mgmt-url')/memos/${memoId}/</activiti:expression>
        </activiti:field>
        <activiti:field name="headers">
          <activiti:string>Accept:application/json</activiti:string>
        </activiti:field>
        <activiti:field expression="memo" name="responseVar"/>
      </extensionElements>
      <incoming>_9</incoming>
      <outgoing>_11</outgoing>
    </serviceTask>
    <scriptTask completionQuantity="1" id="_4" isForCompensation="false" name="Cache &#10;template" scriptFormat="JavaScript" startQuantity="1">
      <incoming>_11</incoming>
      <outgoing>_13</outgoing>
      <script><![CDATA[execution.setVariable('plainContent',execution.getVariable('memo').getString('plainContent'));
execution.setVariable('richContent',execution.getVariable('memo').getString('richContent'));
var contactId = execution.getVariable('contactId');
if (contactId.indexOf('://')==-1) contactId = "userInfo('cust-mgmt-url')/${tenantId}/contacts/"+contactId;
execution.setVariable('contactId',contactId);
]]></script>
    </scriptTask>
    <sequenceFlow id="_11" sourceRef="getMemo" targetRef="_4"/>
