    <serviceTask activiti:async="true" activiti:class="com.knowprocess.resource.spi.RestGet" completionQuantity="1" id="getMemo" implementation="##WebService" isForCompensation="false" name="Read memo" startQuantity="1">
      <extensionElements>
        <activiti:field name="resourceUsername">
          <activiti:string>userInfo('tenant-bot')</activiti:string>
        </activiti:field>
        <activiti:field name="resourcePassword">
          <activiti:expression>userInfo('cust-mgmt-secret')</activiti:expression>
        </activiti:field>
        <activiti:field name="globalResource">
          <activiti:expression>userInfo('cust-mgmt-url')/${tenantId}/memos/${memoName}</activiti:expression>
        </activiti:field>
        <activiti:field name="headers">
          <activiti:string>Accept:application/json</activiti:string>
        </activiti:field>
        <activiti:field expression="memo" name="responseVar"/>
      </extensionElements>
      <incoming>_9</incoming>
      <outgoing>_11</outgoing>
    </serviceTask>
    <scriptTask completionQuantity="1" id="_4" isForCompensation="false" name="Cache template" scriptFormat="JavaScript" startQuantity="1">
      <incoming>_11</incoming>
      <outgoing>_12</outgoing>
      <script><![CDATA[execution.setVariable('plainContent',execution.getVariable('memo').getString('plainContent'));
execution.setVariable('richContent',execution.getVariable('memo').getString('richContent'));
var contactId = execution.getVariable('contactId');
if (contactId.indexOf('://')==-1) contactId = "userInfo('cust-mgmt-url')/"+contactId;
execution.setVariable('contactId',contactId);]]></script>
    </scriptTask>
    <sequenceFlow id="_11" sourceRef="getMemo" targetRef="_4"/>
    <sequenceFlow id="_9" sourceRef="_2" targetRef="getMemo"/>
    <callActivity calledElement="AddActivityToContact" completionQuantity="1" id="addActivityToContact" isForCompensation="false" name="Add activity to contact" startQuantity="1">
      <extensionElements>
        <activiti:in source="contact" target="contact"/>
        <activiti:in sourceExpression="email" target="activityType"/>
        <activiti:in sourceExpression="Memo: ${memo.getString('name')}" target="activityContent"/>
        <activiti:in source="contactId" target="contactId"/>
        <activiti:out source="activityId" target="activityId"/>
      </extensionElements>
      <incoming>_5</incoming>
      <outgoing>_14</outgoing>
    </callActivity>
    <serviceTask activiti:async="true" activiti:type="mail" completionQuantity="1" id="sendMail" implementation="##WebService" isForCompensation="false" name="Send mail" startQuantity="1">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${contact.getString('email')}</activiti:expression>
        </activiti:field>
        <activiti:field name="bcc">
          <activiti:expression>tim@omny.link</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[${memo.getString('title')}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="htmlVar">
          <activiti:expression>richContent</activiti:expression>
        </activiti:field>
        <activiti:field name="textVar">
          <activiti:expression>plainContent</activiti:expression>
        </activiti:field>
      </extensionElements>
      <incoming>_13</incoming>
      <outgoing>_5</outgoing>
    </serviceTask>
    <serviceTask activiti:class="com.knowprocess.resource.spi.RestGet" completionQuantity="1" id="getContact" implementation="##WebService" isForCompensation="false" name="Read contact" startQuantity="1">
      <extensionElements>
        <activiti:field name="resourceUsername">
          <activiti:string>userInfo('tenant-bot')</activiti:string>
        </activiti:field>
        <activiti:field name="resourcePassword">
          <activiti:expression>userInfo('cust-mgmt-secret')</activiti:expression>
        </activiti:field>
        <activiti:field name="globalResource">
          <activiti:expression>${contactId}</activiti:expression>
        </activiti:field>
        <activiti:field name="headers">
          <activiti:string>Accept:application/json</activiti:string>
        </activiti:field>
        <activiti:field expression="contact" name="responseVar"/>
      </extensionElements>
      <incoming>_12</incoming>
      <outgoing>_13</outgoing>
    </serviceTask>
    <sequenceFlow id="_5" sourceRef="sendMail" targetRef="addActivityToContact"/>
    <sequenceFlow id="_13" sourceRef="getContact" targetRef="sendMail"/>
    <sequenceFlow id="_12" sourceRef="_4" targetRef="getContact"/>
    <sequenceFlow id="_14" sourceRef="addActivityToContact" targetRef="endEvent"/>
    <endEvent id="endEvent" name="End with mail sent">
      <incoming>_14</incoming>
      <inputSet/>
    </endEvent>
    <textAnnotation id="_7" textFormat="text/plain">
      <text>Send memo to a single contact</text>
    </textAnnotation>
  </process>
