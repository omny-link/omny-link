    <ioSpecification>
      <dataInput id="_4" isCollection="false" name="contactId"/>
      <dataInput id="_5" isCollection="false" name="contactEmail"/>
      <dataInput id="_6" isCollection="false" name="contactPwd"/>
      <dataInput id="_7" isCollection="false" name="contactRole"/>
      <dataInput id="_9" isCollection="false" name="wordPressSiteUrl"/>
