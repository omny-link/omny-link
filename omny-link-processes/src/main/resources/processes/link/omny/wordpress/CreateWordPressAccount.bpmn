          <!-- WordPress rejects User Agents it does not recognise, possibly to do with the robots setting on dev site -->
          <activiti:string>User-Agent:Mozilla/5.0</activiti:string>
        </activiti:field>
        <activiti:field name="outputVar">
          <activiti:expression>textResponse</activiti:expression>
        </activiti:field>
      </extensionElements>
      <incoming>_3</incoming>
