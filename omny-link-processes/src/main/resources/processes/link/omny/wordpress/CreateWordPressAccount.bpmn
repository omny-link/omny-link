  <process id="CreateWordPressAccount" isClosed="false" isExecutable="true" name="Create WordPress Account" processType="None">
    <documentation id="CreateWordPressAccount_D_1" textFormat="text/plain"><![CDATA[Create a new WordPress account for the contact.]]></documentation>
    <extensionElements>
      <yaoqiang:pageFormat height="842.4" imageableHeight="832.4" imageableWidth="587.6" imageableX="5.0" imageableY="5.0" orientation="0" width="597.6"/>
