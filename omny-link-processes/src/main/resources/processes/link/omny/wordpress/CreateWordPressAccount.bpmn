      <extensionElements>
        <activiti:field name="globalResource">
          <activiti:expression>${wordPressSiteUrl}/wp-admin/admin-ajax.php</activiti:expression>
        </activiti:field>
        <activiti:field name="data">
          <activiti:expression>action=aa_register,log=${contactEmail},user_api_id=${contactId},pwd=${contactPwd}</activiti:expression>
        </activiti:field>
