    <extensionElements>
      <activiti:executionListener class="com.knowprocess.bpm.listeners.FieldInjectionListener" event="start">
        <activiti:field name="varName" stringValue="waitDuration"/>
        <activiti:field expression="${'P1D'}" name="dynamicValue"/>
